{
  "name": "git-coco",
  "version": "0.14.8",
  "description": "zero-effort git commits with coco.",
  "author": "gfargo <ghfargo@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.mjs",
  "files": [
    "dist",
    "!dist/dts"
  ],
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/gfargo/coco#readme",
  "bugs": {
    "url": "https://github.com/gfargo/coco/issues"
  },
  "bin": {
    "coco": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gfargo/coco.git"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "tsx watch src/index.ts",
    "prebuild": "npm run build:schema && npm run build:info",
    "build": "rollup -c",
    "build:schema": "tsx bin/generateSchema.ts",
    "build:info": "tsx bin/generateBuildInfo.ts",
    "build:watch": "rollup -c -w",
    "clean": "rimraf dist && rimraf coverage",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "npm run test:jest && npm run test:publish",
    "test:publish": "npm run lint && npm publish --dry-run",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "release": "release-it",
    "prepublishOnly": "npm run clean && npm run build",
    "coco": "npm run coco:ts",
    "coco:ts": "tsx src/index.ts",
    "coco:cjs": "node dist/index.js",
    "coco:esm": "node dist/index.esm.mjs"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/async": "^3.2.20",
    "@types/chunk-text": "^1.0.0",
    "@types/common-tags": "^1.8.1",
    "@types/diff": "^7.0.1",
    "@types/ini": "^4.1.1",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.10",
    "@types/node": "^22.7.5",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.54.0",
    "eslint-formatter-pretty": "^6.0.0",
    "jest": "^29.7.0",
    "jest-mock": "^29.5.0",
    "release-it": "^17.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^3.26.1",
    "rollup-plugin-banner2": "^1.2.2",
    "rollup-plugin-bin": "^1.0.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-executable": "^1.6.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "ts-jest": "^29.1.0",
    "ts-json-schema-generator": "^2.3.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "tsx": "^4.16.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@inquirer/prompts": "3.3.0",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@langchain/anthropic": "^0.3.5",
    "@langchain/community": "^0.3.4",
    "@langchain/core": "^0.3.13",
    "@langchain/ollama": "^0.1.0",
    "@langchain/openai": "^0.3.5",
    "ajv": "^8.16.0",
    "ajv-formats": "^3.0.1",
    "chalk": "4.1.2",
    "diff": "7.0.0",
    "ini": "5.0.0",
    "minimatch": "10.0.1",
    "ora": "5.4.1",
    "p-queue": "5.0.0",
    "performance-now": "2.1.0",
    "pretty-ms": "7.0.1",
    "simple-git": "3.27.0",
    "tiktoken": "^1.0.17",
    "yargs": "17.7.2"
  }
}
