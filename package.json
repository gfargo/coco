{
  "name": "git-coco",
  "version": "0.21.1",
  "description": "zero-effort git commits with coco.",
  "author": "gfargo <ghfargo@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.mjs",
  "files": [
    "dist",
    "!dist/dts"
  ],
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/gfargo/coco#readme",
  "bugs": {
    "url": "https://github.com/gfargo/coco/issues"
  },
  "bin": {
    "coco": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gfargo/coco.git"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "tsx watch src/index.ts",
    "prebuild": "npm run build:schema && npm run build:info",
    "build": "rollup -c",
    "build:schema": "tsx bin/generateSchema.ts",
    "build:info": "tsx bin/generateBuildInfo.ts",
    "build:watch": "rollup -c -w",
    "clean": "npx rimraf dist && npx rimraf coverage",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "npm run test:jest && npm run test:publish",
    "test:publish": "npm run lint && npm publish --dry-run",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "release": "release-it",
    "prepublishOnly": "npm run clean && npm run build",
    "coco": "npm run coco:ts",
    "coco:ts": "tsx src/index.ts",
    "coco:cjs": "node dist/index.js",
    "coco:esm": "node dist/index.esm.mjs"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/diff": "^8.0.0",
    "@types/ini": "^4.1.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.8",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.54.0",
    "jest": "^30.0.5",
    "release-it": "^19.0.4",
    "rollup": "^3.26.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-executable": "^1.6.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "ts-jest": "^29.1.0",
    "ts-json-schema-generator": "^2.3.0",
    "ts-node": "^10.9.1",
    "tsx": "^4.16.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/core": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@inquirer/prompts": "3.3.0",
    "@langchain/anthropic": "^0.3.14",
    "@langchain/community": "^0.3.32",
    "@langchain/core": "^0.3.40",
    "@langchain/ollama": "^0.2.0",
    "@langchain/openai": "^0.5.0",
    "ajv": "^8.16.0",
    "chalk": "4.1.2",
    "diff": "8.0.2",
    "ini": "5.0.0",
    "minimatch": "^9.0.5",
    "ora": "5.4.1",
    "p-queue": "5.0.0",
    "performance-now": "2.1.0",
    "pretty-ms": "7.0.1",
    "simple-git": "3.28.0",
    "tiktoken": "^1.0.21",
    "yargs": "17.7.2"
  },
  "resolutions": {
    "cross-spawn": "7.0.6"
  }
}
